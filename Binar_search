def binary_search(arr, item):
    low = 0 
    high = len(arr) - 1

    while low <= high:
        mid = (low + high) // 2
        guess = arr[mid]
        print(mid)
        if guess == item:
            return mid
        elif guess > item:
            high = mid - 1 
        else:
            low = mid + 1
    return None

my_list = []

for i in range(0, 1_000_000):
    my_list.append(i)


print(binary_search(my_list, 535600)) 
